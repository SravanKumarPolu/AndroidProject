{
  "name": "impulsevault",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --project tsconfig.test.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "android:emulator": "bash scripts/start-emulator.sh",
    "android:build": "bash scripts/build-and-install.sh",
    "android:sync": "npx expo prebuild --platform android --clean",
    "build:android:preview": "npx eas build --platform android --profile preview",
    "build:android:production": "npx eas build --platform android --profile production",
    "build:android:dev": "npx eas build --platform android --profile development",
    "ios:sync": "npx expo prebuild --platform ios --clean",
    "build:ios:preview": "npx eas build --platform ios --profile preview",
    "build:ios:production": "npx eas build --platform ios --profile production",
    "build:ios:dev": "npx eas build --platform ios --profile development",
    "build:web": "npx expo export --platform web",
    "build:web:production": "npx expo export --platform web",
    "eas:login": "npx eas login",
    "eas:whoami": "npx eas whoami",
    "test:analytics": "echo 'See USER_TESTING_GUIDE.md for analytics testing'",
    "test:prompts": "echo 'See USER_TESTING_GUIDE.md for smart prompts testing'",
    "check:branding": "node scripts/check-branding.js",
    "check:bundle-size": "node scripts/check-bundle-size.js",
    "accessibility:audit": "node scripts/run-accessibility-audit.js",
    "fix:favicon": "bash scripts/fix-favicon.sh",
    "web:clean": "rm -rf .expo/web .expo/cache node_modules/.cache && npm start -- --web --clear",
    "generate:icons": "bash scripts/generate-icon.sh"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@supabase/supabase-js": "^2.81.1",
    "date-fns": "^3.0.0",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.0",
    "expo-dev-client": "~4.0.29",
    "expo-file-system": "~17.0.1",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~6.3.0",
    "expo-location": "^19.0.7",
    "expo-notifications": "~0.28.0",
    "expo-router": "~3.5.0",
    "expo-sharing": "~12.0.1",
    "expo-status-bar": "~1.12.0",
    "expo-store-review": "~7.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.0",
    "react-native": "0.74.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.16.0",
    "react-native-reanimated": "~3.10.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.0",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "sentry-expo": "~7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eas-cli": "^16.27.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.0",
    "lint-staged": "^16.2.7",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "private": true,
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
